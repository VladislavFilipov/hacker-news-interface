{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "printWidth": 80,
  "arrowParens": "avoid",
  "importOrder": [
    "^react(.*)",
    "<THIRD_PARTY_MODULES>",
    "^@src/(.*)$",
    "^@src/(.*).styles.tsx$",
    "^@src/(.*).styled.tsx$",
    "^[./](.*)$",
    "^[../](.*)$"
  ],
  "importOrderSeparation": true
}
